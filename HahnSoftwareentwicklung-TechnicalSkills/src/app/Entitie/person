

export class Person {
    personId:number;
    personName:string;
    personPhone:number;
    personAddress:string;
    personMaritalStatus:string;
    
    constructor(personId:number,personName:string,personPhone:number,
        personAddress:string,personMaritalStatus:string){
            this.personId=personId;
            this.personName=personName;
            this.personPhone=personPhone;
            this.personAddress=personAddress;
            this.personMaritalStatus=personMaritalStatus;
    }
    toString() {
        return this.personId ? `${this.personId}`
        : "";
    }
}